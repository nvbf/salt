{
  "name": "salt",
  "scripts": {
    "dev": "nodemon -w src/ --exec 'yarn transpile-server && yarn dev-start'",
    "transpile-server": "babel src --out-dir dist",
    "dev-start": "node --trace-warnings dist/server",
    "postinstall": "yarn build",
    "build": "next build && yarn transpile-server",
    "start": "NODE_ENV=production node dist/server",
    "lint": "eslint --max-warnings=0 src components pages",
    "test": "yarn lint",
    "precommit": "lint-staged && npm run lint",
    "analyze": "ANALYZE=1 next build && cat .next/stats.txt",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@material-ui/core": "^1.0.0",
    "@sendgrid/mail": "^6.2.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "braintree": "2.11.0",
    "braintree-dropin-react": "^0.1.0",
    "braintree-web-drop-in": "^1.11.0",
    "circular-json": "^0.5.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "downshift": "^1.31.15",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^2.0.0",
    "jest": "^23.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mssql": "^4.1.0",
    "next": "^6.0.3",
    "nodemon": "^1.17.5",
    "npm-check-updates": "^2.14.2",
    "path-match": "^1.2.4",
    "prettier": "1.12.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-safe": "^1.1.1",
    "regenerator-runtime": "^0.11.1",
    "rollbar": "^2.4.0",
    "route-cache": "^0.4.4",
    "sendgrid": "^5.2.3",
    "styled-components": "^3.3.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-bundle-size-analyzer": "^2.7.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write '(src|components|pages)/**/*.js'",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addon-viewport": "^3.4.5",
    "@storybook/addons": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0",
    "babel-runtime": "^6.26.0",
    "flow-bin": "^0.73.0"
  }
}
